%html
  %head
    %title jQuery => Backbone
    %script{:src => '/js/lib/jquery.js', :type => 'text/javascript'}
    %script{:src => '/js/lib/bootstrap.min.js', :type => 'text/javascript'}
    %script{:src => '/js/lib/underscore.js', :type => 'text/javascript'}
    %script{:src => '/js/lib/underscore.string.js', :type => 'text/javascript'}
    %script{:src => '/js/lib/haml.js', :type => 'text/javascript'}
    %link{:rel => 'stylesheet', :type => 'text/css', :href => '/css/bootstrap.min.css'}
    %link{:rel => 'stylesheet', :type => 'text/css', :href => '/css/app.css'}

  %body
    #haml-templates
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          %a.btn.btn-navbar{'data-target' => '.nav-collapse', 'data-toggle' => 'collapse'}
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.brand{:href => '/'} jQuery => Backbone
          .nav-collapse
            %ul.nav
              %li.active
                %a{:href => '/jquery'} jQuery
              %li
                %a{:href => '/backbone'} Backbone
              %li
                %a{:href => 'https://github.com/flanker/from_jquery_to_backbone'} Github
    .container.main-container
      = yield

    = yield_content :javascript
